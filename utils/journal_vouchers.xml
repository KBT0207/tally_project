<?xml version="1.0" encoding="UTF-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>Journal Vouchers</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVCURRENTCOMPANY>COMPANY_NAME_HERE</SVCURRENTCOMPANY>
                <SVFROMDATE>20250101</SVFROMDATE>
                <SVTODATE>20251231</SVTODATE>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <COLLECTION NAME="JournalVouchers">
                        <TYPE>Voucher</TYPE>
                        <FETCH>*</FETCH>
                        <FILTER>IsJournalVoucher</FILTER>
                    </COLLECTION>

                    <SYSTEM TYPE="Formulae" NAME="IsJournalVoucher">
                        $$IsJournal:$VoucherTypeName
                    </SYSTEM>

                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>
