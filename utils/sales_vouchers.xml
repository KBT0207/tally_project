            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <COLLECTION NAME="SalesVouchers">
                        <TYPE>Vouchers</TYPE>
                        <FETCH>*</FETCH>
                        <FILTER>IsSalesVoucher</FILTER>
                    </COLLECTION>
                    <SYSTEM TYPE="Formulae" NAME="IsSalesVoucher">$$IsSales:$VoucherTypeName</SYSTEM>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>