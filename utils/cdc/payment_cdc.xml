<?xml version="1.0" encoding="UTF-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>Payment Vouchers</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVCURRENTCOMPANY>COMPANY_NAME_HERE</SVCURRENTCOMPANY>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <COLLECTION NAME="PaymentVouchers">
                        <TYPE>Voucher</TYPE>
                        <FILTER>IsPaymentVoucher</FILTER>
                        <FILTER>FilterByAlterID</FILTER>

                        <FETCH>GUID</FETCH>
                        <FETCH>ALTERID</FETCH>
                        <FETCH>MASTERID</FETCH>
                        <FETCH>VOUCHERNUMBER</FETCH>
                        <FETCH>VOUCHERTYPENAME</FETCH>
                        <FETCH>DATE</FETCH>
                        <FETCH>REFERENCE</FETCH>
                        <FETCH>NARRATION</FETCH>
                        <FETCH>ALLLEDGERENTRIES.LEDGERNAME</FETCH>
                        <FETCH>ALLLEDGERENTRIES.AMOUNT</FETCH>
                    </COLLECTION>

                    <SYSTEM TYPE="Formulae" NAME="IsPaymentVoucher">$$IsPayment:$VoucherTypeName</SYSTEM>
                    <SYSTEM TYPE="Formulae" NAME="FilterByAlterID">PLACEHOLDER_ALTER_ID</SYSTEM>

                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>