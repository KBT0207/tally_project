<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Collection</TYPE>
        <ID>IncrementalLedgers</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVCURRENTCOMPANY>COMPANY_NAME_HERE</SVCURRENTCOMPANY>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <COLLECTION NAME="IncrementalLedgers" ISMODIFY="No">
                        <TYPE>Ledger</TYPE>
                        <FILTER>AlterIdFilter</FILTER>

                        <FETCH>GUID</FETCH>
                        <FETCH>ALTERID</FETCH>
                        <FETCH>MASTERID</FETCH>
                        <FETCH>NAME</FETCH>
                        <FETCH>PARENT</FETCH>
                        <FETCH>ALIAS</FETCH>
                        <FETCH>EMAIL</FETCH>
                        <FETCH>WEBSITE</FETCH>
                        <FETCH>LEDGERPHONE</FETCH>
                        <FETCH>LEDGERMOBILE</FETCH>
                        <FETCH>LEDGERFAX</FETCH>
                        <FETCH>LEDGERCONTACT</FETCH>
                        <FETCH>PINCODE</FETCH>
                        <FETCH>STATENAME</FETCH>
                        <FETCH>COUNTRYNAME</FETCH>
                        <FETCH>OPENINGBALANCE</FETCH>
                        <FETCH>CREDITLIMIT</FETCH>
                        <FETCH>BILLCREDITPERIOD</FETCH>
                        <FETCH>INCOMETAXNUMBER</FETCH>
                        <FETCH>PARTYGSTIN</FETCH>
                        <FETCH>GSTREGISTRATIONTYPE</FETCH>
                        <FETCH>VATTINNUMBER</FETCH>
                        <FETCH>SALESTAXNUMBER</FETCH>
                        <FETCH>BANKACCHOLDERNAME</FETCH>
                        <FETCH>IFSCODE</FETCH>
                        <FETCH>BRANCHNAME</FETCH>
                        <FETCH>SWIFTCODE</FETCH>
                        <FETCH>BANKIBAN</FETCH>
                        <FETCH>EXPORTIMPORTCODE</FETCH>
                        <FETCH>MSMEREGNUMBER</FETCH>
                        <FETCH>ISBILLWISEON</FETCH>
                        <FETCH>ISDELETED</FETCH>
                        <FETCH>CREATEDDATE</FETCH>
                        <FETCH>ALTEREDON</FETCH>
                        <FETCH>ADDRESS.LIST</FETCH>
                        <FETCH>LANGUAGENAME.LIST</FETCH>
                    </COLLECTION>
                    <SYSTEM TYPE="Formulae" NAME="AlterIdFilter">
                        $ALTERID > PLACEHOLDER_ALTER_ID
                    </SYSTEM>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>