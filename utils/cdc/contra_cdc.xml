<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Collection</TYPE>
        <ID>IncrementalContraVouchers</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVCURRENTCOMPANY>COMPANY_NAME_HERE</SVCURRENTCOMPANY>
                <SVFROMDATE>PLACEHOLDER_FROM_DATE</SVFROMDATE>
                <SVTODATE>PLACEHOLDER_TO_DATE</SVTODATE>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <COLLECTION NAME="IncrementalContraVouchers">
                        <TYPE>Voucher</TYPE>
                        <CHILDOF>$$VchTypeContra</CHILDOF>
                        <FILTER>AlterIdFilter</FILTER>

                        <FETCH>GUID</FETCH>
                        <FETCH>ALTERID</FETCH>
                        <FETCH>MASTERID</FETCH>
                        <FETCH>VOUCHERNUMBER</FETCH>
                        <FETCH>VOUCHERTYPENAME</FETCH>
                        <FETCH>DATE</FETCH>
                        <FETCH>REFERENCE</FETCH>
                        <FETCH>NARRATION</FETCH>

                        <!-- Ledger Entry Fields -->
                        <FETCH>ALLLEDGERENTRIES.LEDGERNAME</FETCH>
                        <FETCH>ALLLEDGERENTRIES.AMOUNT</FETCH>
                    </COLLECTION>
                    <SYSTEM TYPE="Formulae" NAME="AlterIdFilter">
                        $ALTERID > PLACEHOLDER_ALTER_ID
                    </SYSTEM>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>